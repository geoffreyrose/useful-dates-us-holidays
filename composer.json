{
    "name": "geoffreyrose/useful-dates-us-holidays",
    "description": "Useful Dates -  US Holidays Extension",
    "keywords": ["PHP", "Laravel", "Useful Dates", "CARBON", "US Holidays"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "geoffreyrose/useful-dates": "^1.0",
        "ext-calendar": "*"
    },
    "autoload": {
        "psr-4": {
            "UsefulDatesUsHolidays\\": "src/UsHolidays"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "geoffreyrose",
            "email": "geoffrey@geoffreyrose.io"
        }
    ],
    "require-dev": {
        "laravel/pint": "^1.20",
        "pestphp/pest": "^4.1"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
